<h1>Search Results</h1>
<% let worksWithPics = [] %>
<% works.forEach(work => { %>
    <% if(work.primaryimageurl != null || work.primaryimageurl != undefined ) { %>
        <% worksWithPics.push(work) %>
        <% } %>
<% }) %>
        
<% worksWithPics.forEach(work => { %>
    <h4>"<%= work.title %>"</h4>
    <img src="<%= work.primaryimageurl %>" width="400px">
    <form method="POST" action="/popular_works">
        <input type="hidden" name="title" value="<%= work.title %>">
        <input type="hidden" name="primaryimageurl" value="<%= work.primaryimageurl %>">
        <input type="submit" value="Add to Popular Works">
    </form>
<% }) %>

