<h1>Artwork: </h1>

<% paintings.forEach(painting => { %>
    <h3><%= painting.title %></h3>
    <form method="GET" action="/works">
      <input type="hidden" value="<%= painting.title %>">
      <input type="hidden" value="<%= painting.primaryimageurl %>">
      <label for="save-input"></label>
      <input type="submit" value="Save this Artwork">
    </form>
    <img src="<%= painting.primaryimageurl %>" alt="<%= painting.title %>" width=400px>
<% }) %>